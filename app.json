{
  "expo": {
    "name": "TempoRun",
    "slug": "TempoRun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "temporun",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFramework": "static"
          },
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72d060b9-073b-4c82-98e8-e82e2cc5c8e9"
      }
    },
    "owner": "allmall"
  }
}
